# Copy this file to .env and fill in the real values in the VPS / deployment environment
NODE_ENV=production
PORT=3333
JWT_SECRET=change-me

# AWS
AWS_BASE_URL=https://your-bucket.s3.amazonaws.com
AWS_BUCKET_NAME=your-bucket
AWS_DEFAULT_REGION=us-east-1
AWS_SECRET_ACCESS_KEY=changeme
AWS_ACCESS_KEY_ID=changeme

# EMAIL (SMTP)
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=user@example.com
EMAIL_PASSWORD=changeme

FRONT_URL=https://frontend.yourdomain.com

# Database (supply via .env or Docker secrets as needed)
DATABASE_URL=mysql://prisma:prisma@mysql:3306/prod
